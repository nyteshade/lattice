{
  "presets": [
    "stage-1",
    "flow",
    ["env", {
      "targets": { "node": "current" },
      "modules": "commonjs",
      "useBuiltIns": true,
      "debug": false
    }]
  ],
  "plugins": [
    ["transform-class-properties", { "spec": true }],
    "transform-runtime",
    "transform-decorators-legacy"
  ],
  "sourceMaps": "inline"
}
